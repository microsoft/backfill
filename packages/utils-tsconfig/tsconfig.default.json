{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "inlineSourceMap": true,
    "inlineSources": false,
    "module": "commonjs",
    "target": "ES2020",
    "lib": ["ES2020"],
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "skipLibCheck": true,
    "preserveWatchOutput": true,
    "baseUrl": ".",
    "types": ["node", "jest"]
  }
}
